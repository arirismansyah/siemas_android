<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_input_grey"
    android:paddingHorizontal="10dp"
    android:paddingVertical="15dp"
    android:layout_marginVertical="5dp"
    >
    <TableRow android:layout_width="match_parent" >
        <TextView
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text="No Urut ART"
            android:layout_marginTop="10dp"
            >
        </TextView>
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14dp"
        android:layout_column="1"
        android:textStyle="bold"
            android:layout_marginTop="10dp"
        android:text=":">
         </TextView>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/nuART"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:enabled="false"
                android:inputType="number"
                android:text="0"
                android:textSize="14dp"
                android:background="@drawable/textedit_line"
                ></com.google.android.material.textfield.TextInputEditText>
            <TextView
                android:id="@+id/nuARTlama"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="''"
                android:textSize="8dp">
            </TextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </TableRow>
    <TableRow>
        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text="Nama ART"

            android:layout_marginTop="10dp"
            >
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text=":"
            android:layout_marginTop="10dp">
        </TextView>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp"
            >

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/namaART"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:text="namaART"
                android:textSize="14dp"
                android:background="@drawable/textedit_line"
                ></com.google.android.material.textfield.TextInputEditText>
            <TextView
                android:id="@+id/namaARTlama"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="''"
                android:textSize="8dp">
            </TextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </TableRow>
    <TableRow android:layout_height="wrap_content"
        android:gravity="center_vertical">
        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text="Pendidikan ART\n(K. Blok.VI Kol.612)"
            >
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text=":">
        </TextView>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="0dp"
            >
            <Spinner
                android:id="@+id/pendidikanART"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginVertical="10dp"
                android:singleLine="false"
                android:spinnerMode="dialog"
                >
            </Spinner>
            <TextView
                android:id="@+id/pendidikanARTlama"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="''"
                android:textSize="8dp">
            </TextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </TableRow>
    <TableRow
        android:layout_height="wrap_content"
        android:gravity="center_vertical">
        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text="Pekerjaan ART\n(KP. Blok.V.A.3)"
            >
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text=":">
        </TextView>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp">

            <Spinner
                android:id="@+id/pekerjaanART"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_marginVertical="10dp"
                android:singleLine="false"
                android:spinnerMode="dialog"
                >
            </Spinner>

            <TextView
                android:id="@+id/pekerjaanARTlama"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="''"
                android:textSize="8dp">
            </TextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </TableRow>
    <TableRow
        android:layout_height="wrap_content"
        android:gravity="center_vertical">
        <TextView
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:textSize="13dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text="Pendapatan ART\n(KP Blok.V.A\nKol 5+6+7)"
            >
        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="14dp"
            android:layout_column="1"
            android:textStyle="bold"
            android:text=":">
        </TextView>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/pendapatanART"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="number"
                android:text="pendapatanART"
                android:textSize="14dp"
                android:background="@drawable/textedit_line"
                ></com.google.android.material.textfield.TextInputEditText>
            <TextView
                android:id="@+id/pendapatanARTlama"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="''"
                android:textSize="8dp">
            </TextView>
        </androidx.appcompat.widget.LinearLayoutCompat>
    </TableRow>
</TableLayout>